{
    "buildCommand": "node build.js",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/:date/:project/:path*",
            "destination": "/:date/:project/:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}
